# LLM Provider API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LangWatch (for tracing and observability)
LANGWATCH_API_KEY=your_langwatch_api_key_here

# Close CRM Integration (optional)
CLOSE_API_KEY=your_close_api_key_here

# NowCerts Integration (optional - use either username/password OR tokens)
# Option 1: Username/Password (will auto-generate tokens)
NOWCERTS_USERNAME=your_nowcerts_username
NOWCERTS_PASSWORD=your_nowcerts_password
# Option 2: Pre-existing tokens (if you already have them)
NOWCERTS_ACCESS_TOKEN=your_nowcerts_access_token
NOWCERTS_REFRESH_TOKEN=your_nowcerts_refresh_token

# FMCSA API (optional - for DOT lookups)
FMCSA_API_KEY=your_fmcsa_api_key_here
